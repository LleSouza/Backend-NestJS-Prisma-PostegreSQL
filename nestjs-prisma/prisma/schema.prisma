
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// criando um modulo para cadastrar usu√°rios
model Cadastro {
  id    String @id @default(uuid()) 
  name  String?
  email String  @unique
  senha String  

  @@map("cadastros")
}



